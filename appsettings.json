{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "CallbackPath": "/signin-oidc",
    "Domain": "{ YOUR DOMAIN }",
    "TenantId": "{ YOUR TENANT }",
    "ClientId": "{ YOUR CLIENT ID }",
    "Audience": "{ YOUR AUDIENCE }",
    "ClientSecret": "{ YOUR CLIENT SECRET }"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
